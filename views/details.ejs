<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="/css/style.css">
        <title>favorite</title>
    </head>
<body>
    <header>
        <div>
            <img src="/img/the_simpsons.png" alt="">
        </div>
        <div>
            <a href="/">favourte qute</a>
            <a href="/favorite-quotes">home</a>
        </div>
    </header>
    <main>
        <div clas="card">
            <% dataArr.forEach(object => { %>
                <img src="<%= object.img %>" alt="">
                <p><%= object.name %></p>
                <form action="/update/<%= object.id %>_method=PUT" method="POST">
                    <input type="text" name="qute" value="<%= object.qute %>">
                    <input type="submit" value="updtae">
                </form>
                <form action="/delete/<%= object.id %>_method=Delete" method="POST">
                    <input type="submit" value="delete">
                </form>
            <% }) %>
        </div>
    </main>
</body>
</html>